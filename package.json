{
  "name": "prettyfox-ui",
  "version": "1.0.16-alpha",
  "description": "Native Angular 2 components styling by bootstrap 4",
  "keywords": [
    "angular2",
    "bootstrap",
    "angularjs",
    "components",
    "prettyfox",
    "twitter-bootstrap"
  ],
  "author": "http://prettyfox.org/",
  "scripts": {
    "start": "gulp build:demo && tsc -w && npm run lite",
    "start:aot": "gulp build:demo-aot && npm run build && tsc -w && npm run lite",
    "prod": "gulp build:demo-aot && tsc && npm run build:prod && tsc -w && npm run lite",
    "lite": "npm run lite",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build": "webpack --config webpack.config.js",
    "build:prod": "webpack --config webpack.config.js --env.MODE=prod"
  },
